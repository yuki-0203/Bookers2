<div class="container">  
<div class="row">     
  <div class="col-lg-4">
    <%= render 'users/user' %>
   </div>
   
<div class="col-lg-8"> 
    <div class="Books">
      <h2>Users</h2>
        <table class="table table-striped">
          <thead>
            <tr>
               <th>image</th>
               <th>name</th>
               <th colspan="3"></th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <% @users.each do |f| %>
                <td><%= attachment_image_tag f, :profile_image, format: 'jpeg', size: '100x100' %></td>
                <td><%=f.name%></td>
                <td><%= link_to'show',user_path(f)%></td>
            </tr>
               <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>  
